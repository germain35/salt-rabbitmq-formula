rabbitmq:
  bind:
    address: 0.0.0.0
    port: 5672
  secret_key: rabbit_master_cookie
  admin:
    name: adminuser
    password: pwd
  vhosts:
    '/monitor':
      enabled: true
      user: 'monitor'
      password: 'password'
      policies:
        - name: HA
          pattern: '^(?!amq\.).*'
          definition: '{"ha-mode": "all"}'
  plugins:
    - rabbitmq_stomp
    - amqp_client